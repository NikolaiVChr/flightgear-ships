<turbine>
   <name>turbine-set</name>
   <mode>looped</mode>
   <path>Aircraft/U.S. NAVY H.Q/vessel-sound/vessel1.wav</path>
   

<condition>
     <property>/controls/engines/engine[0]/throttle</property>
     <less-than>
       <property>/engines/engine[0]/throttle</property>
       <value>5</value>
     </less-than>
   </condition>
 <volume>
    <property>ai/models/aircraft[0]/velocities/true-airspeed-kt</property>
    <factor>0.022</factor>
    <min>0.0000000008</min>
    <max>0.58</max>
    
   </volume> 
  
  <pitch><property>ai/models/aircraft[0]/velocities/true-airspeed-kt</property>
  <factor>0.04</factor><min>0.5</min><max>6.5</max><offset>0.15</offset></pitch> </turbine>



<cruise>

<name>stamping</name>
   <mode>looped</mode>
   <path>Aircraft/U.S. NAVY H.Q/vessel-sound/vessel.wav</path>
   

<condition>
     <property>ai/models/aircraft[0]/velocities/true-airspeed-kt</property>
       </condition>
   <volume>
    <property>ai/models/aircraft[0]/velocities/true-airspeed-kt</property>
    <factor>1</factor>
    <min>0.000000008</min>
    <max>30.08</max>
    
   </volume>   
  <pitch>
    <property>ai/models/aircraft[0]/velocities/true-airspeed-kt</property>

    <factor>0.0000028</factor>
    <min>0.00003</min>
    <max>1.25</max>
    <offset>0.3</offset>
   </pitch> 
  </cruise>



<cruisen>
   <name>stamp2</name>
   <mode>looped</mode>
   <path>Aircraft/U.S. NAVY H.Q/vessel-sound/vessel.wav</path>
   <condition>
    <property>/engines/engine[0]/throttle</property>   
   </condition>
 <volume>
  <property>/orientation/alpha-deg</property> 
  <type>log</type> 
  <min>0.5</min> 
  <max>2.0</max> 
  </volume>
  <volume>
    <property>/engines/engine[0]/throttle</property><factor>0.25</factor><min>2</min><max>5</max></volume>
  </cruisen>

 
<horn>
