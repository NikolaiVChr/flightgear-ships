<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

 
 

 
 
  <key n="76">
    <name>L</name>
    <binding>
	<command>property-toggle</command>
	<property>controls/lighting/landzone</property>
	</binding>
	<binding><command>property-toggle</command>
	<property>controls/lighting/coord-light</property>
	</binding></key>

	
	<key n="115">
        <name>s</name>
        <desc>slow </desc>
        <binding>
          <command>property-assign</command>
      <property>/controls/engines/engine[0]/throttle</property>
	  <value>0.26</value>
         <condition><greater-than>
<property>/controls/engines/engine[0]/throttle</property>
	  <value>0.009</value>
      </greater-than></condition>
        </binding>
      </key>
	
	
	<key n="72">
        <name>H</name>
        <desc>half</desc>
        <binding>
          <command>property-assign</command>
      <property>/controls/engines/engine[0]/throttle</property>
	  <value>0.52</value>
          <condition><greater-than>
<property>/controls/engines/engine[0]/throttle</property>
	  <value>0.2</value>
      </greater-than></condition>
        </binding>
      </key>
	
     <key n="67">
        <name>C</name>
        <desc>cruise speed</desc>
        <binding>
          <command>property-assign</command>
      <property>/controls/engines/engine[0]/throttle</property>
	  <value>0.75</value>
	  	  <condition><greater-than>
<property>/controls/engines/engine[0]/throttle</property>
	  <value>0.2</value>
      </greater-than></condition>
        </binding>
      </key>

	  
	  <key n="102">
        <name>f</name>
        <desc>full ahead</desc>
        <binding>
          <command>property-assign</command>
      <property>/controls/engines/engine[0]/throttle</property>
	  <value>1</value>
	  <condition><greater-than>
         <property>/controls/engines/engine[0]/throttle</property>
	  <value>0.2</value>
      </greater-than></condition>
        </binding>
      </key>

	  
	  <key n="32"><name>space</name>
        <desc>full stop</desc>
        <binding>
          <command>property-assign</command>
      <property>/controls/engines/engine[0]/throttle</property>
	  <value>0.009</value>
	  	  <condition><less-than>
         <property>/controls/engines/engine[0]/mixture</property>
	  <value>1</value>
      </less-than></condition>
        </binding>
      </key>

	  
	  <key n="127"><name>del</name>
        <desc>reverse</desc>
        <binding>
          <command>property-cycle</command>
					<property>/controls/engines/engine[0]/reverser</property>
					<value>true</value>
					<value>false</value>
        </binding>
      </key>

 

  <key n="361">
        <name>PageDown</name>
    <repeatable type="bool">true</repeatable>
    <desc>throttle fac-</desc>
    <binding>
      <command>property-adjust</command>
      <property>/controls/engines/engine[0]/throttle</property>
      <step type="double">-0.001421</step>
    </binding>
  </key>

  <key n="360">
        <name>PageUp</name>
    <repeatable type="bool">true</repeatable>
    <desc>throttle fac+</desc>
    <binding>
      <command>property-adjust</command>
      <property>/controls/engines/engine[0]/throttle</property>
      <step type="double"> 0.001421</step>
	  <condition>
   <property>/controls/engines/engine[0]/throttle</property>
	  <value>0.009</value>
      </condition>
    </binding>
  </key>

  <!--key n="127"><name>del</name>
  </key>
  
    <key n="32"><name>space</name>
  </key-->
  
</PropertyList>
